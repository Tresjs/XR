{
  "name": "@tresjs/xr",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@9.7.1",
  "license": "MIT",
  "keywords": [
    "vue",
    "3d",
    "threejs",
    "three",
    "threejs-vue",
    "XR",
    "AR",
    "VR",
    "physics"
  ],
  "maintainers": [
    "Alvaro Saburido (https://github.com/alvarosabu/)",
    "Wade Penistone (https://github.com/Truemedia/)"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/tresxr.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "./dist/tresxr.js",
  "module": "./dist/tresxr.js",
  "types": "./dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "cd playground && pnpm dev",
    "playground": "cd playground && pnpm dev",
    "build": "vite build",
    "release": "release-it",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "prepare": "husky"
  },
  "peerDependencies": {
    "@tresjs/core": ">=3.5.0",
    "three": ">=0.158.0",
    "vue": ">=3.3.8"
  },
  "dependencies": {
    "@tresjs/core": "^4.0.2",
    "pinia": "^2.1.7",
    "three": "^0.165.0",
    "vue": "^3.4.30"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^8.0.1",
    "@tresjs/eslint-config": "^1.1.0",
    "@types/node": "^20.14.9",
    "@types/three": "^0.165.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "dts": "^0.1.1",
    "eslint": "^9.5.0",
    "eslint-plugin-vue": "^9.26.0",
    "husky": "^9.0.11",
    "kolorist": "^1.8.0",
    "pathe": "^1.1.2",
    "release-it": "^17.4.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "typescript": "^5.5.2",
    "unocss": "^0.63.0",
    "vite": "^5.3.1",
    "vite-plugin-banner": "^0.7.1",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-mkcert": "^1.17.5",
    "vitepress": "1.4.1",
    "vue-tsc": "^2.0.22"
  }
